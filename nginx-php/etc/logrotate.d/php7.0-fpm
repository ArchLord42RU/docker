/data/logs/php7.0-fpm.log {
    rotate 12
    weekly
    missingok
    notifempty
    compress
    delaycompress
    postrotate
        /usr/lib/php/php7.0-fpm-reopenlogs
    endscript
}
/data/logs/php-fpm-daemon.log {
    rotate 12
    weekly
    missingok
    notifempty
    compress
    delaycompress
    postrotate
        /usr/lib/php/php7.0-fpm-reopenlogs
    endscript
}