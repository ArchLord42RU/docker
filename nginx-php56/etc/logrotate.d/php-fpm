/data/logs/php-fpm.log {
    rotate 12
    weekly
    missingok
    notifempty
    compress
    delaycompress
    postrotate
        /usr/lib/php/php-fpm-reopenlogs
    endscript
}
/data/logs/php-fpm-daemon.log {
    rotate 12
    weekly
    missingok
    notifempty
    compress
    delaycompress
    postrotate
        /usr/lib/php/php-fpm-reopenlogs
    endscript
}